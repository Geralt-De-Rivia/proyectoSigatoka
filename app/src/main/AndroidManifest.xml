<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rocky_geralt.sigatoka">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_myicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_myicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuPrincipal" android:label="@string/title_activity_menu_principal" android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Sigatoka" android:parentActivityName=".MenuPrincipal" android:label="@string/layoutSigatoka"></activity>
        <activity android:name=".Estadios" android:parentActivityName=".MenuPrincipal" android:label="@string/layoutEstadios" ></activity>
        <activity android:name=".Prevenir" android:parentActivityName=".MenuPrincipal" android:label="@string/layoutPrevenir"></activity>
        <activity android:name=".Sennova" android:parentActivityName=".MenuPrincipal" android:label="@string/layoutSennova"></activity>
    </application>

</manifest>